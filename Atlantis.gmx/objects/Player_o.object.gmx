        <arguments>
          <argument>
            <kind>1</kind>
            <string>l_key= keyboard_check_direct(vk_left);
r_key= keyboard_check_direct(vk_right);
d_key= keyboard_check_direct(vk_down);
u_key= keyboard_check_direct(vk_up);
j_key= keyboard_check_direct(vk_space);

if(wait==0){
    if(inWater){
        if(u_key){
         v_speed=-10;
         if(onGround){
             v_speed*=4;
             onGround=false;
            }
        }
        if(d_key){
            v_speed=9.5;
        }
        if(r_key){//right
            h_speed=9.5;
            image_xscale=1;
        }
        if(l_key){//left
         h_speed=-9.5;
         image_xscale=-1;
        }
        if((d_key&amp;&amp;u_key)||(!d_key&amp;&amp;!u_key)){
            v_speed=0;
        }
        if(abs(h_speed)&gt;0){
            if(h_speed&gt;0)
                h_speed-=waterRes;
            else if(h_speed&lt;0)
                h_speed+=waterRes;
        }
    
        //slow the vertical speed       
        //check for horizontal collision
        if place_meeting(x+h_speed,y,block){
            while(!place_meeting(x+sign(h_speed),y,block))
                 x+=sign(h_speed);
            h_speed=0;        
        }
        else if(place_meeting(x,y,kracken)){
            h_speed=-15;
            hp=hp-10;
        }
        else if(place_meeting(x,y,Octoeel)){
            h_speed*=0.5;
            v_speed*=0.5;
        }
        //check for vertical
        v_speed+=grav;
        if place_meeting(x,y+v_speed,block){
            if(v_speed&gt;=0){
                while(!place_meeting(x,y+1,block))
                    y++;
                v_speed=0;
                onGround=true;
            }
            else{
                while(!place_meeting(x,y-1,block))
                    y--;
                v_speed=0;
            }
        }
        //moving
    
        x+=h_speed;
        y+=v_speed;
    }
    else{
        //no water and has to walk       
        if(l_key){
            h_speed=-5;
            image_xscale=-1;
        }
        if(r_key){
            h_speed=5;
            image_xscale=1;
        }
        if((l_key&amp;&amp;r_key)||(!l_key&amp;&amp;!r_key))
            h_speed=0;
        if(j_key){
            if(onGround)
                v_speed=-20;
        }
        if(u_key){
            v_speed=-2.5;
        }
        
        
        v_speed+=grav;
        if place_meeting(x,y+v_speed,block){
            if(v_speed&gt;=0){
                while(!place_meeting(x,y+1,block))
                    y++;
                v_speed=0;
                onGround=true;
            }
            else{
                while(!place_meeting(x,y-1,block))
                    y--;
                v_speed=0;
            }
        }
        if place_meeting(x+h_speed,y,block){
            while(!place_meeting(x+sign(h_speed),y,block))
                x+=sign(h_speed);
            h_speed=0;        
        }
////////////////////////////////////////////////////////////////
        if(attached){//its not slowingdown
            h_speed*=0.25;
            v_speed*=0.25;
        }
////////////////////////////////////////////////////////////////
        x+=h_speed;
        y+=v_speed;
    }
}
else
    wait--;
    
//spring board speed up to simulate soe swimming then slow daon from the resistance
